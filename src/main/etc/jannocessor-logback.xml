<?xml version="1.0" encoding="UTF-8"?>
<configuration>
 
    <appender name="console" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%-30(%date) %-5level %logger{0} %msg%n</pattern>
        </encoder>
    </appender>
 
    <root level="error">
        <appender-ref ref="console"/>
    </root>
 
</configuration>